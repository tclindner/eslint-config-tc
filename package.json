    "eslint-plugin-unicorn": "^50.0.1",
    "is-plain-obj": "^3.0.0",
    "jest": "^29.7.0",
    "npm-package-json-lint": "^7.1.0",
    "npm-package-json-lint-config-tc": "^7.0.0",
    "prettier": "^3.1.1"
