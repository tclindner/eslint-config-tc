    "eslint-plugin-unicorn": "^31.0.0",
    "is-plain-obj": "^3.0.0",
    "jest": "^27.0.6",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-tc": "^4.1.0",
    "prettier": "^2.3.2"
