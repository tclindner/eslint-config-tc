    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "is-plain-obj": "^3.0.0",
    "jest": "^29.4.2",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-tc": "^6.0.0",
    "prettier": "^2.8.4"
