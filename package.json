{
  "name": "eslint-config-tc",
  "version": "2.0.0",
  "description": "ESLint shareable config for TC's projects",
  "keywords": [
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "code style"
  ],
  "homepage": "https://github.com/tclindner/eslint-config-tc#readme",
  "bugs": {
    "url": "https://github.com/tclindner/eslint-config-tc/issues"
  },
  "author": "Thomas Lindner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tclindner/eslint-config-tc.git"
  },
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --format=node_modules/eslint-formatter-pretty",
    "lint": "npm run npmpackagejsonlint && npm run eslint",
    "npmpackagejsonlint": "pjl-cli -c ./.npmpackagejsonlintrc.json",
    "test": "mocha"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-formatter-pretty": "^1.1.0",
    "is-plain-obj": "^1.1.0",
    "mocha": "^3.4.2",
    "npm-package-json-lint": "^2.5.0",
    "npm-package-json-lint-config-tc": "^1.0.0",
    "temp-write": "^3.2.0"
  },
  "peerDependencies": {
    "eslint": "^4.3.0"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.14.7"
  },
  "license": "MIT"
}
